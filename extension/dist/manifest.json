{
    "manifest_version": 3,
    "name": "Checkmade",
    "description": "We made a check for you.",
    "version": "1.0.0",
    "icons": {
        "16": "extension.png",
        "48": "32.png",
        "128": "extension.png"
    },
    "action": {
        "default_title": "Add to Data!",
        "default_popup": "newTab.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage", 
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}